# Clave secreta del agente (se genera al crear el agente desde el frontend)
# Esta clave se usa para autenticar el agente con el backend
CLAVE_SECRETA=

# URL del backend (REST API)
BACKEND_URL=https://lector-mediciones-backend.onrender.com

# Intervalo de polling para detectar cambios de configuración (ms)
# Default: 30000 (30 segundos)
CONFIG_POLL_INTERVAL_MS=30000

# Modo de operación Modbus: "real" o "simulado"
MODO_MODBUS=real

# Interfaz de usuario: "web" o "terminal"
INTERFAZ=web
